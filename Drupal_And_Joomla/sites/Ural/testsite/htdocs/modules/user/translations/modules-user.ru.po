msgid "edit role"
msgstr "изменить роль"

#: modules/user/user.module:2022
msgid "permission"
msgstr "разрешение"

#: modules/user/user.module:2225
msgid "Low"
msgstr "Низкий"

#: modules/user/user.module:2227
